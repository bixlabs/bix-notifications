{
  "name": "bix-notifications",
  "version": "1.1.1",
  "description": "Notification module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=5.11.0"
  },
  "scripts": {
    "build": "npm run lint && npm run compile",
    "prepublish": "npm run build",
    "lint": "eslint src",
    "test": "echo \"To be implemented\"",
    "compile": "babel src --out-dir dist"
  },
  "babel": {
    "presets": [
      "es2017",
      "es2016",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "cucumber"
    ],
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "comma-dangle": 0,
      "generator-star-spacing": 0,
      "no-use-before-define": 0,
      "object-curly-spacing": 0,
      "consistent-return": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bixlabs/bix-notifications.git"
  },
  "keywords": [
    "bixlabs",
    "notifications"
  ],
  "author": "Edd Su√°rez <eddsuarez89@gmail.com> (https://github.com/eddsuarez)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EddSuarez/notifications/issues"
  },
  "homepage": "https://github.com/EddSuarez/notifications#readme",
  "dependencies": {
    "debug": "^2.6.2",
    "fs": "0.0.1-security",
    "mongoose": "^4.7.8",
    "path": "^0.12.7",
    "socket.io": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^6.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-cucumber": "^0.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0"
  }
}
